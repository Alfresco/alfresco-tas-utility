<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Sanity Suite" parallel="classes">
	<parameter name="testGroup" value="sanity" />
	<listeners>
		<listener class-name="org.alfresco.utility.listener.SanityTestsGeneratorListener" />
	</listeners>
	<test name="Sanity Tests">
		<packages>
			<package name="org.alfresco.utility.*" />
		</packages>
	</test>
</suite> 
